
<% content_for :navbar do %>
  <div id = "header">
       <%= render partial:"partials/navbar", locals:{array:[{'Sing up' => '#'},{'Log in'=>new_user_session_path}], back: unauthenticated_root_path}%>
    </div>
<% end %>

<%= bootstrap_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>
<div>
  <div class="field">
    <%= f.text_field :name, autofocus: true, autocomplete: "name", skip_label: true, placeholder: "Name" %>
  </div>
  <div class="field">
    <%= f.email_field :email, autofocus: true, autocomplete: "email", skip_label: true, placeholder: "Email" %>
  </div>

  <div class="field">
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "new-password", skip_label: true, placeholder: "Password" %>
  </div>

  <div class="field">
    <%= f.password_field :password_confirmation, autocomplete: "new-password", skip_label: true, placeholder: "Confirm password" %>
  </div>

</div>
  

  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>

 

<% end %>

<%= render "devise/shared/links" %>
